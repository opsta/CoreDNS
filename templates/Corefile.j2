dev.tel.internal {
    etcd {
        path /skydns
        endpoint http://localhost:2379
    }
    prometheus
    cache
    loadbalance
}

.:53 {
    errors
    health {
        lameduck 5s
    }
    ready
    prometheus :9153
    forward . tls://1.1.1.1 tls://1.0.0.1 {
    	tls_servername cloudflare-dns.com
	    health_check 5s
    }
    cache 30
    loop
    reload
    loadbalance
}